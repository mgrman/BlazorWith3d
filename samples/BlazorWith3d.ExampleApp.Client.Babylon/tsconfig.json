{
    "compileOnSave": true,
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "ES6",
        "outDir": "wwwroot/js",
        "moduleResolution": "node"
    },
    "include": [
        "scripts/**/*"
    ],
    "exclude": [
        "wwwroot"
    ]
}